    agent testing{
        stages {
            stage('npm-build') {
                steps {
                nodejs(nodeJSInstallationName: 'Node 17.x', configId: '<config-file-provider-id>') {
                    sh 'npm config ls'
                    sh 'npm --version'
                }

                steps{
                    sh 'npm --version'
                    sh 'node --version'
                }
            }
        }
        
    }

}
