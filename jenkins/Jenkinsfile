pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }
        
        stage('Use Node.js') {
            steps {
                nodejs(nodeJSInstallationName: 'Node 14.x', configId: '<config-file-provider-id>') {
                    sh 'npm config ls'
                }
            }
        }
    }

}
